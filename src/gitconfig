[color]
  ui = auto

[push]
  default = current

[alias]
  br    = branch
  ci    = commit
  co    = checkout

  di    = diff
  dc    = diff --cached
  ds    = diff --staged

  st    = status

  fml   = !git aa && git reset --hard
  pop   = !git stash pop
  subs  = submodule update --init --recursive
  lg    = "!. ~/.githelpers && pretty_git_log"
